<!DOCTYPE html>
<html>
    <head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta http-equiv="X-UA-Compatible" content="ie=edge">
		{{ wp_head() }}
    </head>

    <body>
	    
		{% block header %}
			{% include 'base/header.twig' %}
		{% endblock %}
        
        <main data-section-id="main" class="wrapper {{wrapper_class}}">
            {% block content %}
            {% endblock %}
        </main>

	
		{% block footer %}
			{% include 'base/footer.twig' %}
		{% endblock %}

		{{ wp_footer() }}
      
    </body>
</html>